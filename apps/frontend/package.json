{
	"name": "@voltage/frontend",
	"private": true,
	"version": "1.1.2",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "vite build",
		"lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
