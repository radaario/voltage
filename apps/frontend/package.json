		"lodash-es": "^4.17.21",
		"react": "^19.1.1",
		"react-dom": "^19.1.1",
		"react-router-dom": "^6.30.1",
		"timeago-react": "^3.0.7"
	},
	"devDependencies": {
		"@eslint/js": "^9.36.0",
		"@playwright/test": "^1.49.1",
		"@tailwindcss/aspect-ratio": "^0.4.2",
